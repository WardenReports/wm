{
    "manifest_version": 2,
    "name": "Work Manager",
    "description": "Work Manager By Warden",
    "version": "1.8.9",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["Content.js"]
      }
    ],
    "icons": { "48": "icon48.png", "128": "icon128.png" },
    "browser_action": {
      "default_popup": "popup.html",
      "default_title": "Work Manager By Warden"
    },
    "permissions": [
      "https://roblox.com",
      "https://serverguard.wixsite.com/warden",
      "https://www.roblox.com.uy"
    ]
  }